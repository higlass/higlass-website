{
  "assets": {
    "images": "assets/images",
    "scripts": "assets/scripts",
    "scriptsThirdParty": "assets/scripts-third-party",
    "styles": "assets/styles",
    "videos": "assets/videos"
  },
  "browsers": [
    "> 1%",
    "last 2 versions",
    "Firefox ESR",
    "Opera 12.1",
    "Explorer >= 8",
    "Firefox >= 3.6"
  ],
  "buildPaths": {
    "development": "dev",
    "production": "dist"
  },
  "dist": "dist",
  "docs": "docs",
  "eslintDevRules": {
    "no-console": 1
  },
  "eslintIgnore": "third-party",
  "jsBundles": {
    "base": {
      "banner": "Global scripts",
      "name": "base"
    },
    "data-sets": {
      "banner": "HiGlass data-sets",
      "name": "data-sets"
    },
    "examples": {
      "banner": "HiGlass Examples",
      "name": "examples"
    },
    "hg-launcher": {
      "banner": "HiGlass Launcher",
      "name": "hg-launcher"
    }
  },
  "markedOptions": {},
  "rollupExternals": [
    "hglib",
    "d3"
  ],
  "rollupGlobals": {
    "hglib": "hglib"
  },
  "server": {
    "host": "localhost",
    "port": "3001"
  },
  "src": "src",
  "styles": [
    "index.scss"
  ],
  "thirdPartyScripts": [
    "node_modules/higlass/dist/scripts/hglib.js"
  ],
  "wiki": "wiki"
}
