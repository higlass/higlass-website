{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "airbnb/base"
  ],
  "parser": "babel-eslint",
  "rules": {
    "comma-dangle": ["error", "never"],
    "import/prefer-default-export": 1,
    "no-underscore-dangle": 0,
    "no-console": 2,
    "prefer-destructuring": 0
  }
}
